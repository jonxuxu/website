generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Lesson {
  id       Int      @id @default(autoincrement())
  createdAt DateTime @default(now()) @db.Timestamp(6)
  lesson   String
}
